  "penalties": {
    "missingRelevantKeywords": {
      "enabled": true,
      "severity": "high",
      "description": "Penalize when critical keywords for the role are missing",
      "penaltyPerMissing": 5
    },
    "keywordStuffing": {
      "enabled": true,
      "repeatThreshold": 6,
      "severity": "medium",
      "description": "Penalize excessive repetition of the same keyword",
      "penaltyPerExcess": 2
    },
    "irrelevantKeywords": {
      "enabled": true,
      "severity": "low",
      "description": "Slight penalty for keywords not matching job role category",
      "penaltyWeight": 1
    },
    "lackOfActionVerbs": {
      "enabled": true,
      "minimumRequired": 3,
      "severity": "low",
      "description": "Encourage use of strong action verbs",
      "penaltyIfBelow": 3
    }
  },
  "bonuses": {
    "certifications": {
      "enabled": true,
      "bonusPerCert": 5,
      "description": "Reward relevant certifications"
    },
    "quantifiedAchievements": {
      "enabled": true,
      "bonusPerMetric": 3,
      "description": "Reward results with numbers (%, $, metrics)",
      "patterns": [
        "\\b\\d{1,3}(?:,\\d{3})+(?:\\.\\d+)?\\b",
        "\\b\\d+(?:\\.\\d+)?%\\b",
        "\\b\\d{2,}\\b",
        "(?:\\$|₹|€|£)\\s*\\d+(?:,\\d{3})*(?:\\.\\d+)?\\s*(?:k|m|b|l|cr)?\\b",
        "\\b\\d+(?:\\.\\d+)?\\s*(?:k|m|b|l|cr)\\b",
        "\\b\\d+\\+\\b",
        "\\b\\d+(?:\\.\\d+)?x\\b",
        "\\b\\d+(?:\\.\\d+)?\\s*(?:ms|s|sec|secs|minutes|min|hours|hr|hrs|days|weeks|months|years)\\b",
        "\\b(?:users|requests|transactions|records|rows|customers|clients)\\b\\s*:\\s*\\d+(?:,\\d{3})*\\b",
        "\\b\\d+(?:,\\d{3})*\\s*(?:users|requests|transactions|records|rows|customers|clients)\\b"
      ]
    },
    "diverseSkillset": {
      "enabled": true,
      "bonusThreshold": 15,
      "bonusAmount": 10,
      "description": "Bonus for having skills across multiple groups"
    }
  },
  "scoring": {
    "maxScore": 100,
    "passingScore": 70,
    "excellentScore": 85,
    "formula": "weighted_keyword_matches + bonuses - penalties"
  }
}
